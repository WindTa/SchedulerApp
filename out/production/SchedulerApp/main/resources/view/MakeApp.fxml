<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTimePicker?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controllers.MakeAppController">
   <children>
      <HBox prefHeight="24.0" prefWidth="560.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Make Appointment">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Text>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="homeButton" mnemonicParsing="false" onAction="#homeClick" prefHeight="25.0" prefWidth="125.0" text="Home" />
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <DatePicker HBox.hgrow="ALWAYS" />
            <JFXTimePicker HBox.hgrow="ALWAYS" />
            <JFXTimePicker HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="25.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox spacing="20.0" HBox.hgrow="ALWAYS">
               <children>
                  <VBox spacing="20.0">
                     <children>
                        <TextField maxWidth="-Infinity" prefHeight="25.0" prefWidth="175.0" />
                        <TextField maxWidth="-Infinity" prefHeight="25.0" prefWidth="175.0" />
                        <Region maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
                        <Button maxWidth="-Infinity" mnemonicParsing="false" prefHeight="25.0" prefWidth="175.0" text="Button" />
                     </children>
                  </VBox>
                  <TextArea maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </HBox.margin>
            </HBox>
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
